{
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"eol-last": "warn",
		"no-duplicate-imports": "warn",
		"object-curly-spacing": ["error", "always"],
		"array-bracket-spacing": ["error", "never"],
		"comma-spacing": "error",
		"no-multiple-empty-lines": ["error", { "max": 1 }],
		"no-trailing-spaces": "warn",
		"object-shorthand": ["error", "always"]
	}
}
